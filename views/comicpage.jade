doctype html
html
	include head
	body
		include header
		if holiday.isChristmasSeason()
			include holidayHeader
		div#loading
			p Grabbing frogs...
			p
				img(src='/simg/ajax-loader.gif')
		div#comicArea
			include comic
		include inputModal
		include footer
		include js
		!= '<!-- Last Update: ' + (new Date(pubDate)).getDate() + '/' + ((new Date(pubDate)).getMonth() + 1) + '/' + (new Date(pubDate)).getFullYear() + ' -->'
